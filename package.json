{
    "name": "registry-app",
    "version": "0.1.0",
    "proxy": "http://localhost:5000",
    "private": true,
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "concurrently": "^4.1.0",
        "cookie-session": "^2.0.0-beta.3",
        "dotenv": "^8.1.0",
        "express": "^4.16.4",
        "http-proxy-middleware": "^0.19.1",
        "morgan": "^1.9.1",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^1.0.0",
        "pg": "^7.8.1",
        "sequelize": "^4.42.1",
        "validator": "^10.11.0"
    },
    "scripts": {
        "db:init": "dropdb --if-exists registry_db && createdb registry_db",
        "db:reset": "node scripts/resetDb.js",
        "db:model": "node models/models.js",
        "db:seed": "node scripts/seed.js",
        "server": "nodemon server.js",
        "dev": "concurrently \"npm run server\" \"react-scripts start\"",
        "start2": "concurrently \"node server.js\" \"react-scripts start\"",
        "start": "node server.js",
        "heroku-postbuild": "npm run build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
